{
  "name": "@balena/jellyfish-action-library",
  "version": "5.1.127",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-action-library.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Action library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js > README.md",
    "postinstall": "if [[ -z \"$CI\" && -d \".git\" ]]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.34",
    "@balena/jellyfish-environment": "^2.3.4",
    "@balena/jellyfish-logger": "^0.0.162",
    "@balena/jellyfish-mail": "^0.0.126",
    "@balena/jellyfish-metrics": "^0.0.189",
    "@balena/jellyfish-sync": "^3.1.21",
    "@balena/jellyfish-uuid": "^1.0.36",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "blueimp-md5": "^2.18.0",
    "date-fns": "^2.16.1",
    "fast-json-patch": "^3.0.0-1",
    "googleapis": "^65.0.0",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.20",
    "request-promise": "^4.2.6",
    "skhema": "^5.3.3",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.0.8",
    "ava": "^3.14.0",
    "catch-uncommitted": "^1.6.2",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.0.4",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
}
